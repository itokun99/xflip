{
  "name": "xflip",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "update:assets": "npx react-native-asset",
    "pod:install": "cd ios && bundle exec pod install --repo-update",
    "gradlew:clean": "cd android && ./gradlew clean && cd ..",
    "build:aab": "npx react-native build-android --mode=release",
    "build:apk": "cd android && ./gradlew assembleRelease && cd ..",
    "build:apk-debug": "cd android && ./gradlew assembleDebug && cd ..",
    "bundle:js": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "fresh-install": "rm -rf node_modules && yarn install"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5",
    "@itokun99/hooks": "^1.0.0",
    "@react-native-clipboard/clipboard": "^1.15.0",
